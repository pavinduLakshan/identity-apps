{
    "name": "dashboard",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/dashboard/src",
    "projectType": "application",
    "generators": {},
    "prefix": "@wso2is",
    "tags": [],
    "implicitDependencies": [
        "react-components"
    ],
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "commands": [
                    "pnpm build"
                ],
                "cwd": "apps/dashboard",
                "parallel": false
            }
        },
        "clean": {
            "executor": "nx:run-commands",
            "options": {
                "commands": [
                    {
                        "command": "pnpm clean",
                        "description": "Cleaning dashboard"
                    }
                ],
                "cwd": "apps/dashboard",
                "parallel": false
            }
        },
        "lint": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm lint",
                "cwd": "apps/dashboard",
                "parallel": false
            }
        },
        "test": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm test",
                "cwd": "apps/dashboard",
                "parallel": false
            }
        },
        "test-coverage": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm test:coverage",
                "cwd": "apps/dashboard",
                "parallel": false
            }
        }
    }
}
